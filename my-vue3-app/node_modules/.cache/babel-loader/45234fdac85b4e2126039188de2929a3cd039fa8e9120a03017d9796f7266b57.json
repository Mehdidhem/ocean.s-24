{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from 'vue';\nimport { ElMessageBox } from 'element-plus';\nconst drawer = ref(false);\nexport default {\n  name: 'ChatApp',\n  data() {\n    return {\n      messages: [],\n      newMessage: ''\n    };\n  },\n  methods: {\n    sendMessage() {\n      if (this.newMessage.trim() !== '') {\n        this.addMessage('text', this.newMessage);\n        this.newMessage = '';\n      }\n    },\n    addMessage(type, content) {\n      this.messages.push({\n        type,\n        content,\n        id: this.messages.length\n      });\n    }\n  }\n};","map":{"version":3,"names":["ref","ElMessageBox","drawer","name","data","messages","newMessage","methods","sendMessage","trim","addMessage","type","content","push","id","length"],"sources":["C:\\Users\\JieZHANG\\ocean.s-24\\my-vue3-app\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <header>\n      <!-- Chat Header -->\n      <h1>Green Data</h1>\n    </header>\n    \n    <div class=\"chat-window\">\n      <!-- Chat messages -->\n      <div class=\"message\" v-for=\"message in messages\" :key=\"message.id\">\n        <div v-if=\"message.type === 'text'\">\n          {{ message.content }}\n        </div>\n        <img v-else-if=\"message.type === 'image'\" :src=\"message.content\" alt=\"Sent Image\" />\n        <video v-else-if=\"message.type === 'video'\" controls>\n          <source :src=\"message.content\" type=\"video/mp4\">\n          Your browser does not support the video tag.\n        </video>\n      </div>\n      \n      <!-- Cat Image -->\n      <img src=\"path_to_cat_image.jpg\" alt=\"Chat Cat\" />\n    </div>\n    \n    <div class=\"chat-input\">\n      <!-- Message Input -->\n      <el-input v-model=\"input\" style=\"width: 240px\" placeholder=\"Please input\" />\n      \n      <!-- Image and Video Upload -->\n      <el-button type=\"primary\" style=\"margin-left: 16px\" @click=\"drawer = true\">\n        open\n      </el-button>\n\n      <el-drawer\n        v-model=\"drawer\"\n        title=\"I am the title\"\n        :direction=\"btt\"\n      >\n        <span>Hi, there!</span>\n      </el-drawer>\n\n      <!-- Send Button -->\n      <el-button type=\"primary\">send</el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from 'vue'\nimport { ElMessageBox } from 'element-plus'\n\nconst drawer = ref(false)\nexport default {\n  name: 'ChatApp',\n  data() {\n    return {\n      messages: [],\n      newMessage: ''\n    };\n  },\n  methods: {\n    sendMessage() {\n      if (this.newMessage.trim() !== '') {\n        this.addMessage('text', this.newMessage);\n        this.newMessage = '';\n      }\n    },\n\n    addMessage(type, content) {\n      this.messages.push({ type, content, id: this.messages.length });\n    }\n  }\n};\n</script>\n\n<style>\n/* Add your CSS styling here */\n.chat-window img, .chat-window video {\n  max-width: 100%;\n  height: auto;\n}\n</style>\n"],"mappings":";AAgDA,SAASA,GAAE,QAAS,KAAI;AACxB,SAASC,YAAW,QAAS,cAAa;AAE1C,MAAMC,MAAK,GAAIF,GAAG,CAAC,KAAK;AACxB,eAAe;EACbG,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAI,IAAI,CAACF,UAAU,CAACG,IAAI,CAAC,MAAM,EAAE,EAAE;QACjC,IAAI,CAACC,UAAU,CAAC,MAAM,EAAE,IAAI,CAACJ,UAAU,CAAC;QACxC,IAAI,CAACA,UAAS,GAAI,EAAE;MACtB;IACF,CAAC;IAEDI,UAAUA,CAACC,IAAI,EAAEC,OAAO,EAAE;MACxB,IAAI,CAACP,QAAQ,CAACQ,IAAI,CAAC;QAAEF,IAAI;QAAEC,OAAO;QAAEE,EAAE,EAAE,IAAI,CAACT,QAAQ,CAACU;MAAO,CAAC,CAAC;IACjE;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}