{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from 'vue';\nimport img from './/assets/logo.png';\nexport default {\n  __name: 'App',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const input = ref('');\n    const drawer = ref(false);\n    const drawer1 = ref(false);\n    const direction = ref('btt');\n    const images = ref([img]);\n    const messages = ref([]);\n    function sendMessage(type, content) {\n      if (type === 'text' && input.value.trim()) {\n        const newMessage = {\n          id: Date.now(),\n          type: type,\n          content: input.value\n        };\n        messages.value.push(newMessage);\n        input.value = '';\n      } else if (type === 'image' && content) {\n        const newMessage = {\n          id: Date.now(),\n          type: type,\n          content: content\n        };\n        messages.value.push(newMessage);\n        drawer.value = false;\n      }\n    }\n    const __returned__ = {\n      input,\n      drawer,\n      drawer1,\n      direction,\n      images,\n      messages,\n      sendMessage,\n      ref,\n      get img() {\n        return img;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","img","input","drawer","drawer1","direction","images","messages","sendMessage","type","content","value","trim","newMessage","id","Date","now","push"],"sources":["C:/Users/JieZHANG/Desktop/ocean.s-24/my-vue3-app/src/pages/user/App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <header>\r\n      <!-- Chat Header -->\r\n      <h1>Green Data</h1>\r\n    </header>\r\n\r\n    <div class=\"chat-window\">\r\n      <!-- Chat messages -->\r\n      <div class=\"chat-window\">\r\n        <div id=\"init-message\" class=\"message\">Salut ! Envoie-moi des messages, photos ou vidéos pour voir le trajet de\r\n          tes données en temps réel !</div>\r\n        <div class=\"message right\" v-for=\"message in messages\" :key=\"message.id\">\r\n          <div v-if=\"message.type === 'text'\">\r\n            {{ message.content }}\r\n          </div>\r\n          <img v-else-if=\"message.type === 'image'\" :src=\"message.content\" alt=\"Sent Image\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"chat-input\">\r\n      <!-- Message Input -->\r\n      <input id=\"inputmsg\" v-model=\"input\" placeholder=\"Écrivez un message…\" />\r\n\r\n      <div class=\"chat-input-buttons\">\r\n        <!-- Image and Video Upload -->\r\n        <button style=\"chat-button\" @click=\"drawer = true\">\r\n          <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"2em\" height=\"em\" viewBox=\"0 0 256 256\">\r\n            <path fill=\"currentColor\"\r\n              d=\"M224 160V48a16 16 0 0 0-16-16H48a16 16 0 0 0-16 16v160a16 16 0 0 0 16 16h160a16 16 0 0 0 16-16v-48ZM208 48v92.7L179.3 112a16.1 16.1 0 0 0-22.6 0L112 156.7L91.3 136a16.1 16.1 0 0 0-22.6 0L48 156.7V48Zm0 160H48v-28.7l32-32l20.7 20.7a16.1 16.1 0 0 0 22.6 0l44.7-44.7l40 40V208ZM91.5 100.5A11.9 11.9 0 0 1 88 92a12 12 0 0 1 24 0a12 12 0 0 1-12 12a12.3 12.3 0 0 1-8.5-3.5Z\">\r\n            </path>\r\n          </svg>\r\n        </button>\r\n        <button style=\"chat-button\" @click=\"drawer1 = true\">\r\n          <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"2em\" height=\"2em\" viewBox=\"0 0 256 256\"><path fill=\"currentColor\" d=\"M244 73.1a7.7 7.7 0 0 0-8 0l-44 25.1V92a40 40 0 0 0-40-40H24A16 16 0 0 0 8 68v96a40 40 0 0 0 40 40h128a16 16 0 0 0 16-16v-30.2l44 25.1a8.3 8.3 0 0 0 4 1.1a8 8 0 0 0 4-1.1a7.8 7.8 0 0 0 4-6.9V80a7.8 7.8 0 0 0-4-6.9ZM176 188H48a24.1 24.1 0 0 1-24-24V68h128a24.1 24.1 0 0 1 24 24v96Zm56-25.8l-40-22.8v-22.8l40-22.8Z\"/></svg>\r\n        </button>\r\n\r\n        <el-drawer v-model=\"drawer\" title=\"Envoyer une image\" :direction=\"direction\">\r\n          <div class=\"image-list\">\r\n            <img v-for=\"img in images\" :src=\"img\" :key=\"img\" @click=\"sendMessage('image', img)\" class=\"image-item\" />\r\n          </div>\r\n        </el-drawer>\r\n\r\n        <el-drawer v-model=\"drawer1\" title=\"Envoyer une vidéo\" :direction=\"direction\">\r\n        </el-drawer>\r\n\r\n        <!-- Send Button -->\r\n        <button class=\"chat-send\" @click=\"sendMessage('text', input.value)\"> Envoyer </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref } from 'vue'\r\nimport img from './/assets/logo.png'\r\n\r\nconst input = ref('')\r\nconst drawer = ref(false)\r\nconst drawer1 = ref(false)\r\nconst direction = ref('btt')\r\nconst images = ref([img])\r\n\r\nconst messages = ref([])\r\n\r\nfunction sendMessage(type, content) {\r\n  if (type === 'text' && input.value.trim()) {\r\n    const newMessage = {\r\n      id: Date.now(),\r\n      type: type,\r\n      content: input.value\r\n    }\r\n    messages.value.push(newMessage)\r\n    input.value = ''\r\n  } else if (type === 'image' && content) {\r\n    const newMessage = {\r\n      id: Date.now(),\r\n      type: type,\r\n      content: content\r\n    }\r\n    messages.value.push(newMessage)\r\n    drawer.value = false\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n@import url('https://fonts.googleapis.com/css2?family=Outfit:wght@100;200;300;400;500;600;700;800;900');\r\n\r\nhtml,\r\nbody {\r\n  margin: 0px;\r\n  padding: 0px;\r\n  font-family: Outfit, sans-serif;\r\n}\r\n\r\n#app {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\nh1 {\r\n  position: fixed;\r\n  background-color: #fff;\r\n  width: 100%;\r\n  top: 0px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 4vh 2vh;\r\n  margin: 0px;\r\n  opacity: .9;\r\n  backdrop-filter: blur(8px);\r\n  font-size: 1.25rem;\r\n}\r\n\r\n.chat-window {\r\n  width: 90vw;\r\n  margin-top: 10vh;\r\n  margin: 6vh auto;\r\n  overflow-y: auto;\r\n  border-radius: 10px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: flex-end;\r\n}\r\n\r\n.message {\r\n  max-width: 60%;\r\n  margin: 4px;\r\n  padding: 12px 16px;\r\n  border-radius: 24px 24px 2px 24px;\r\n  background-color: #ebeaea;\r\n  word-wrap: break-word;\r\n  font-size: 1rem;\r\n  font-weight: 300;\r\n}\r\n\r\n#init-message {\r\n  border-radius: 24px 24px 24px 2px;\r\n  background-color: #9747ff;\r\n  color: white;\r\n  align-self: flex-start;\r\n}\r\n\r\n.chat-input {\r\n  padding: 2vh 2vh 4vh 2vh;\r\n  display: flex;\r\n  gap: 16px;\r\n  align-items: center;\r\n  justify-content: center;\r\n  position: fixed;\r\n  bottom: 0px;\r\n  width: calc(100% - 2vh - 16px);\r\n  background-color: #fff;\r\n}\r\n\r\n.chat-input-buttons {\r\n  display: flex;\r\n}\r\n\r\n.chat-button {\r\n  display: block;\r\n}\r\n\r\n.chat-send {\r\n  display: block;\r\n  font-family: \"Outfit\";\r\n  font-weight: 700;\r\n  color: #4600a2;\r\n}\r\n\r\nbutton {\r\n  background-color: transparent;\r\n  border: none;\r\n  height: 48px;\r\n  width: 48px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  opacity: .75;\r\n}\r\n\r\n.image-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  height: fit-content;\r\n}\r\n\r\n.image-item {\r\n  width: 100px;\r\n  margin: 16px;\r\n  cursor: pointer;\r\n}\r\n \r\ninput {\r\n  border-radius: 100px;\r\n  padding: 16px 18px;\r\n  border: none;\r\n  background-color: #ebeaea;\r\n  color: #000000;\r\n  width: 100%;\r\n  font-family: Outfit;\r\n}\r\n\r\nimg {\r\n  border-radius: 16px;\r\n}\r\n\r\n\r\n\r\n</style>\r\n"],"mappings":";AAuDA,SAASA,GAAG,QAAQ,KAAK;AACzB,OAAOC,GAAG,MAAM,oBAAoB;;;;;;;IAEpC,MAAMC,KAAK,GAAGF,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMG,MAAM,GAAGH,GAAG,CAAC,KAAK,CAAC;IACzB,MAAMI,OAAO,GAAGJ,GAAG,CAAC,KAAK,CAAC;IAC1B,MAAMK,SAAS,GAAGL,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMM,MAAM,GAAGN,GAAG,CAAC,CAACC,GAAG,CAAC,CAAC;IAEzB,MAAMM,QAAQ,GAAGP,GAAG,CAAC,EAAE,CAAC;IAExB,SAASQ,WAAWA,CAACC,IAAI,EAAEC,OAAO,EAAE;MAClC,IAAI